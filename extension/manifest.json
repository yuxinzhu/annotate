{
  "manifest_version": 2,

  "name": "Annotate",
  "description": "This extension allows users to annotate highlighted text on a page",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["reset.css", "index.css"],
      "js": ["jquery-1.11.1.min.js", "index.js"]
    }
  ]
}
